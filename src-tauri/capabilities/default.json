{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:path:default",
    "opener:default",
    "fs:default",
    {
      "identifier": "fs:scope",
      "allow": ["$APPDATA/**"]
    }
  ]
}
